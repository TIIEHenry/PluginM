<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.testhost"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".HostApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HostMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- <activity android:name="com.example.testplugin.PluginMainActivity"> -->
        <!-- </activity> -->
        <activity android:name=".HostActivityA">
        </activity>
        <activity
            android:name=".HostActivityB"
            android:process=":b">
        </activity>

        <service
            android:name=".HostService"
            android:process=":back">
        </service>

        <provider
            android:name=".HostContentProvider"
            android:authorities="com.example.testhost.provider"
            android:exported="false">
        </provider>
    </application>

</manifest>